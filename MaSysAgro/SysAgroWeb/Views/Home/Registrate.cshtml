@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Sign up</title>
    <link href="/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="/Content/style.css" rel="stylesheet" />
    <link href="/Content/img/icono-pest.ico" rel="icon" />
</head>
<body class="body1">
    <div class="container">
        <div class="row">
            <div class="col-lg-3"></div>
            <div class="col-lg-6">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <br /><br />
                      

                    </div>
                </div>
                <div class="card card-login">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-lg-12 text-center">
                                <br />
                                <h2 style="color:#7ab37f">Sign up</h2>
                                <br />
                            </div>
                        </div>
                        <div class="mb-3 padding1">
                            <label for="email" class="form-label label-login">User :</label>
                            <input id="txtUsuario" class="form-control form-login" type="text" name="name" value="" />
                        </div>
                        <div class="mb-3 mt-3 padding1">
                            <label for="email" class="form-label label-login">Email :</label>
                            <input id="txtCorreo" class="form-control form-login" type="text" name="name" value="" />
                        </div>
                        <div class="mb-3 mt-3 padding1">
                            <label for="email" class="form-label label-login">First Name :</label>
                            <input id="txtFirstName" class="form-control form-login" type="text" name="name" value="" />
                        </div>
                        <div class="mb-3 mt-3 padding1">
                            <label for="email" class="form-label label-login">Last Name :</label>
                            <input id="txtLastName" class="form-control form-login" type="text" name="name" value="" />
                        </div>
                        <div class="mb-3 mt-3 padding1">
                            <label for="email" class="form-label label-login">Password :</label>
                            <input id="txtContrasena" class="form-control form-login" type="password" name="name" value="" />
                        </div>
                        <div class="mb-3 mt-3 padding1">
                            <label for="email" class="form-label label-login">Verify Password :</label>
                            <input id="txtVerifyContrasena" class="form-control form-login" type="password" name="name" value="" />
                        </div>
                        <div class="mb-3 mt-3 padding1">
                            <label for="" class="" style="color:red;display:none;" id="txtErrror"></label>
                        </div>
                        <div class="mb-3 mt-3 text-center" >
                            <button id="btnCrearCuenta" class="btn btn-primary" type="button" name="name" value="">
                                Create Account
                            </button>
                        </div>
                        <br />
                        <br />

                    </div>
                </div>
            </div>
            <div class="col-lg-3"></div>

        </div>

    </div>
    <script src="~/Scripts/js/Global.js"></script>
    <script src="~/Scripts/axios.js"></script>
    <script src="~/Scripts/sweetalert2.all.min.js"></script>
    <script src="~/Scripts/axios.min.js"></script>
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/bootstrap.bundle.min.js"></script>
    <script src="~/Scripts/js/Login/RegistrateControllers.js?v=1.0"></script>
    <script type="text/javascript">
        (function () {
            var clsRegistrateControllers = new RegistrateControllers();
            clsRegistrateControllers.Inicializar();
        })();
    </script>
</body>

</html>
